
DIR_STM32F103ZET6			:=	$(DIR_PLATFORM)/STM32F103ZET6
DIR_STM32F103ZET6_SUBDIRS	:=

STM32F103ZET6_ASM		:=		$(wildcard $(DIR_STM32F103ZET6)/*.s)
STM32F103ZET6_INC		:=		-I$(DIR_STM32F103ZET6)
STM32F103ZET6_LIB		:=		-L$(DIR_STM32F103ZET6)
STM32F103ZET6_LD		+=		$(wildcard $(DIR_STM32F103ZET6)/*.ld)

BUILD_LIB				+=		$(STM32F103ZET6_LIB)

CC_STATICLIB			+=		
OUTPUT_ASM			+=		$(STM32F103ZET6_ASM)
OUTPUT_INC			+=		$(STM32F103ZET6_INC)
OUTPUT_LIB			+=		$(STM32F103ZET6_LIB)
OUTPUT_OBJ			+=		$(STM32F103ZET6_OBJ)

LINK_LD				+=		$(STM32F103ZET6_LD)


DES_LIB_STM32F103ZET6		:=		$(DIR_OUT_LIB)/libplatform.a
DES_LIB						+=		$(DES_LIB_STM32F103ZET6)
DES_LIB_NAME				+=		platform

$(DIR_OUT_OBJ)/$(DIR_STM32F103ZET6)/%.o:$(DIR_STM32F103ZET6)/%.s
	@ echo '  [AS]  ' $<
	@ mkdir -p $(DIR_OUT_OBJ)/$(DIR_STM32F103ZET6)
	@ $(AS)  $< -o $@ $(OUTPUT_INC)

$(DES_LIB_STM32F103ZET6):$(DIR_STM32F103ZET6)/libplatform.a
	@ echo '  [CP]  ' $<
	@ mkdir -p $(DIR_OUT_LIB)
	@ cp $< $@ -rf

	